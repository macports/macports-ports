            return -code error
            }
   } elseif {[string match *-17 ${subport}] && ${os.major} < 18}  {
        known_fail yes
        pre-fetch {
            ui_error "${name} ${version} is only supported on Mac OS X 10.14 Mojave or later."
            return -code error
            }
   } elseif {[string match *-17-graalvm-ce ${subport}] && ${os.major} < 18}  {
        known_fail yes
        pre-fetch {
