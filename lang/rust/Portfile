subport rust-src {
    # remove dependencies
    depends_build
    depends_lib

    set rust_source_dir ${destroot}${prefix}/lib/rustlib/src/rust

    description     Source code for the rust programming language
    long_description {*}${description}

    use_configure no

    build {}

    destroot {
        xinstall -d ${rust_source_dir}
        move ${worksrcpath}/src ${rust_source_dir}/src
        move ${worksrcpath}/library ${rust_source_dir}/library

        # correct the permissions
        system -W ${rust_source_dir} "find . -type d -exec chmod 755 {} \\;"
        system -W ${rust_source_dir} "find . -type f -exec chmod 644 {} \\;"

        # delete the test directories (which for some god awful reason contains binaries)
        system -W ${rust_source_dir} "find . -type d -name test -print0 | xargs -0 rm -rf"
    }
}

# The root cause of offline mode is prevent cargo to check signatures
# which is broken before 10.8
# See: https://github.com/rust-lang/cargo/issues/4813
# See: https://github.com/mozilla/releases-comm-central/commit/5cd3595203de78a6f47324f53f0cf616f4e383ea
if {${subport} ne "${ccwrap}" } {
    PortGroup           cargo_fetch 1.0

    dist_subdir         $name

    post-extract {
        # enable offline mode for cargo at bootstrap
        system -W ${worksrcpath} "patch -p0 < ${filespath}/offline-bootstrap.patch"
    }

    if { ${os.platform} eq "darwin" && ${os.major} < 12 } {
        # I don't need whole libcxx
        # anyway, it is an easy way to get some missed symbols from compiler-rt
        legacysupport.use_mp_libcxx yes

        # well, let me build a small library which contains only needed symbols
        set scalpel_dylib ${workpath}/scalpel.dylib
        pre-build {
            set export_list_file ${workpath}/scalpel.export_list
            # set scalpel_dylib ${workpath}/scalpel.dylib
            set export_list [open ${export_list_file} w]

            # Since https://github.com/rust-lang/rust/commit/8a6ff90a3a41e6ace18aeb089ea0a0eb3726dd08
            puts ${export_list} "___muloti4"

            close ${export_list}

            system "${configure.cc} -dynamiclib -exported_symbols_list ${export_list_file} -o ${scalpel_dylib} ${prefix}/lib/libcxx/libc++.a"

            build.env-append \
                        DYLD_FORCE_FLAT_NAMESPACE=1 \
                        DYLD_INSERT_LIBRARIES=${scalpel_dylib}

        }

        # inside destroot it also calls cargo / rustc
        destroot.env-append \
                        DYLD_FORCE_FLAT_NAMESPACE=1 \
                        DYLD_INSERT_LIBRARIES=${scalpel_dylib}
    }

    # use cargo2port to update it
    # cargo from rust bootstart is linked agains system's crypto library
    # and old system may not support required SSL/TLS version to fetch anything from github for example.

