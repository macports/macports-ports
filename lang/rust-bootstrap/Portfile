                            --set=build.cargo=${cargo_prefix}/cargo/bin/cargo \
                            --set=target.${run_platform}.cc=[compwrap::wrap_compiler cc] \
                            --set=target.${run_platform}.cxx=[compwrap::wrap_compiler cxx] \
                            --set=target.${run_platform}.linker=[compwrap::wrap_compiler ld] \
                            --set=target.${run_platform}.ar=${rust.archiver} \
                            --set=target.${run_platform}.ranlib=${rust.ranlib}

foreach arch ${muniversal.architectures} {
    if { ${arch} eq ${configure.build_arch} } { continue }
    set platform            [option triplet.${arch}]
    configure.args.${arch}-append \
                            --set=target.${platform}.cc=[compwrap::wrap_compiler cc] \
                            --set=target.${platform}.cxx=[compwrap::wrap_compiler cxx] \
                            --set=target.${platform}.linker=[compwrap::wrap_compiler ld] \
                            --set=target.${platform}.ar=${rust.archiver} \
                            --set=target.${platform}.ranlib=${rust.ranlib}
}

# this part gets a little tricky
# we want to limit the number of LLVM targets to reduce build times
# however, we need to include the target for *both* the host and build systems
# see https://trac.macports.org/ticket/67595
#    aarch64-apple-darwin  is Tier 1
#    host could be x86_64-apple-darwin
if { ${os.arch} eq "arm" } {
    configure.args.arm64-append \
                            --set=llvm.targets=AArch64
} else {
    configure.args.arm64-append \
                            --set=llvm.targets=AArch64\\\;X86
}
#    x86_64-apple-darwin is Tier 1
#    host could be aarch64-apple-darwin
if { ${os.arch} eq "arm" } {
    configure.args.x86_64-append \
                            --set=llvm.targets=X86\\\;AArch64
} else {
    configure.args.x86_64-append \
                            --set=llvm.targets=X86
}
#    i686-apple-darwin is Tier 3, so build and host are alwasy different
#    host is always x86_64
configure.args.i386-append  --set=llvm.targets=X86
#    see https://lists.llvm.org/pipermail/llvm-dev/2016-October/thread.html#106359
configure.args.ppc-append   --set=llvm.targets=PowerPC\\\;X86
configure.args.ppc64-append --set=llvm.targets=PowerPC\\\;X86
if {${configure.ccache}} {
    configure.args-append   --enable-ccache
} else {
    configure.args-append   --disable-ccache
}
# see
#     https://github.com/rust-lang/rust/blob/340f6491bed3525acfabbdbd1545b6aee2fca62b/src/bootstrap/lib.rs#L1033
#     https://github.com/rust-lang/rust/blob/340f6491bed3525acfabbdbd1545b6aee2fca62b/compiler/rustc_llvm/build.rs#L329
configure.cxx_stdlib        libc++
configure.args-append       --enable-use-libcxx

build.cmd                   ${configure.python} x.py
build.target                dist
build.args                  --jobs=${build.jobs} cargo rustc rust-std

test.run                    yes
test.target                 test
test.args                   --stage 2

rust::append_envs           RUST_BACKTRACE=1

set rust_source_dir         ${prefix}/libexec/rust/src

# do not include build information in installed libraries
# see https://github.com/rust-lang/rust/blob/master/src/bootstrap/bin/rustc.rs
# see also rust-src subport
build.env-append            RUSTC_DEBUGINFO_MAP=${workpath}=${rust_source_dir}

destroot {
    xinstall -d -m 0755 ${destroot}${prefix}/libexec/${name}

    if { ${subport} ne "${name}-transition" } {
        set vendor macports
    } else {
        set vendor transition
    }

    if { ${subport} eq "${name}-10.6" } {
        set major   10
    } elseif { ${subport} eq "${name}-10.5" } {
        set major   9
    } elseif { ${subport} eq "${name}-10.4" } {
        set major   8
    } else {
        set major   ""
    }

    foreach tool {cargo rust-std rustc} {
        xinstall -m 0644 \
            ${destroot.dir}/build/dist/${tool}-${version}-[option triplet.${muniversal.build_arch}]${extract.suffix} \
            ${destroot}${prefix}/libexec/${name}/${tool}-${version}+${revision}-[option triplet.cpu.${muniversal.build_arch}]-${vendor}-${triplet.os}${major}${extract.suffix}
    }
}

