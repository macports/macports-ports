post-extract {
    # ensure std library can be found by rust binary
    foreach std_dir [glob -types d -directory ${workpath} -tails rust-std-*] {
        set run_platform    [join [lrange [split ${std_dir} -] end-2 end] -]
        set rustc_dir       rustc-[join [lrange [split ${std_dir} -] 2 end] -]
        ln -s               ${workpath}/${std_dir}/rust-std-${run_platform}/lib/rustlib/${run_platform}/lib \
                            ${workpath}/${rustc_dir}/rustc/lib/rustlib/${run_platform}/
    }
}

patchfiles-append           patch-offline_bootstrap.diff \
                            patch-configure.py.diff \
                            patch-i686_to_i386.diff
if { ${subport} ne "${name}-transition" } {
    patchfiles-append       patch-emulate_tls.diff
} else {
    patchfiles-append       patch-force_emulate_tls.diff
}
if { ${rust.use_cctools} } {
    patchfiles-append       patch-llvm_libtool.diff
    post-patch {
        reinplace           "s|__MACPORTS_PREFIX__|${prefix}|g" \
                            ${worksrcpath}/src/bootstrap/native.rs
    }
}
if { ${os.platform} eq "darwin" && [vercmp ${macosx_deployment_target} < 10.6] } {
    # CFURLSetResourcePropertyForKey was introduced in 10.6, so turn off the (optional) function that uses it
    patchfiles-append       patch-no_CFURLSetResourcePropertyForKey.diff
}
if { ${os.platform} eq "darwin" && [vercmp ${macosx_deployment_target} < 10.10] } {
    # revert https://github.com/llvm/llvm-project/commit/9902a0945d22cd5757b16ebe85fe07059723aa09
    # qos.h was introduced in 10.10
    patchfiles-append       patch-no_qos.diff
}

configure.cmd               ${configure.python} src/bootstrap/configure.py

if { ${os.platform} eq "darwin" && [vercmp ${macosx_deployment_target} < 10.7] } {
    # Apple Clang does not use emulated thread-local-storage on older systems
    compiler.blacklist-append   clang
}

if { ${os.platform} eq "darwin" && [vercmp ${macosx_deployment_target} < 10.8] } {
    # ___emutls_get_address and ___muloti4
    # see https://github.com/rust-lang/rust/commit/8a6ff90a3a41e6ace18aeb089ea0a0eb3726dd08
    rust.add_compiler_runtime   yes
}

configure.args-append       --disable-codegen-tests \
                            --disable-docs \
                            --disable-vendor \
                            --disable-dist-src \
                            --set=build.verbose=2 \
                            --release-channel=stable \
                            --disable-extended \
                            --experimental-targets="" \
                            --llvm-cflags="${rust.llvm.cflags}" \
                            --llvm-cxxflags="${rust.llvm.cxxflags}" \
                            --llvm-ldflags="${rust.llvm.ldflags}" \
                            --python=${configure.python}
