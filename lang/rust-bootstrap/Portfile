destroot {
    xinstall -d -m 0755 ${destroot}${prefix}/libexec/${name}

    if { ${subport} ne "${name}-transition" } {
        set vendor macports
    } else {
        set vendor transition
    }

    if { ${subport} eq "${name}-10.6" } {
        set major   10
    } elseif { ${subport} eq "${name}-10.5" } {
        set major   9
    } elseif { ${subport} eq "${name}-10.4" } {
        set major   8
    } else {
        set major   ""
    }

    foreach tool {cargo rust-std rustc} {
        xinstall -m 0644 \
            ${destroot.dir}/build/dist/${tool}-${version}-[option triplet.${muniversal.build_arch}]${extract.suffix} \
            ${destroot}${prefix}/libexec/${name}/${tool}-${version}+${revision}-[option triplet.cpu.${muniversal.build_arch}]-${vendor}-${triplet.os}${major}${extract.suffix}
    }
}

