depends_build-append        port:cargo-c
if {${configure.build_arch} in {i386 x86_64}} {
    depends_build-append    port:nasm
}

pre-build {
    # Cargo.lock does not exist
    system -W ${worksrcpath} "${prefix}/bin/cargo update"
}

post-build {
    system -W ${worksrcpath} "${prefix}/bin/cargo cbuild --release --frozen --prefix ${prefix} --destdir ${destroot}"
}

destroot {
    system -W ${worksrcpath} "${prefix}/bin/cargo cinstall --release --frozen --prefix ${prefix} --destdir ${destroot}"
}

# the copious releases marked "Weekly pre-release" hides the current release in the default livecheck
# the "Latest release" is an alpha release
livecheck.url               https://api.github.com/repos/xiph/rav1e/releases
livecheck.regex             \"tag_name\": \"v(\\d+(?:\\.\\d+)*)\"

