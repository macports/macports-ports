+{
+	random_pool_ = seed;
+	random_seed_ = seed;
+	for(random_calls_ = 0; random_calls_ < call_count; ++random_calls_) {
+		random_next();
+	}
+	if(fallback_to_legacy_rng())
+		legacy_rng_.seed_random(seed, call_count);
