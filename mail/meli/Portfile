livecheck.regex     {\"(alpha-[^\"]+[0-9])\"}

destroot {
    xinstall -m 0755 \
        ${build.dir}/target/[cargo.rust_platform]/release/${name} \
        ${destroot}${prefix}/bin/

    xinstall -d ${destroot}${prefix}/share/man/man1
    xinstall -d ${destroot}${prefix}/share/man/man5

    copy {*}[glob ${build.dir}/docs/*.1] ${destroot}${prefix}/share/man/man1/
    copy {*}[glob ${build.dir}/docs/*.5] ${destroot}${prefix}/share/man/man5/

    xinstall -d ${destroot}${prefix}/share/${name}
    copy ${build.dir}/docs/samples ${destroot}${prefix}/share/${name}/
}

