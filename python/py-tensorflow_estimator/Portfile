if {${name} ne ${subport}} {
    conflicts   py${python.version}-tensorflow_estimator1

    depends_build-append \
        port:py${python.version}-pip

    if {${python.version} eq 27} {
        depends_lib-append \
            port:py${python.version}-futures
    }

    build {
    }

    destroot.cmd        pip-${python.branch}
    destroot.args       \
        --ignore-installed \
        --no-cache-dir \
        --no-dependencies \
        --root ${destroot} \
        ${distpath}/${distfiles}
    destroot.post_args

    livecheck.type  none
