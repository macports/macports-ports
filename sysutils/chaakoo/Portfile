go.setup            github.com/pallavJha/chaakoo 0.0.1-beta v
revision            0

description         Slice and dice your TMUX windows and panes

long_description    {*}${description}. ${name} is a wrapper over TMUX that \
                    can create sessions, windows and panes from a grid based \
                    layout. The idea here is inspired by the CSS grid \
                    template areas.

categories          sysutils
installs_libs       no
license             Apache-2
maintainers         {gmail.com:herby.gillot @herbygillot} \
                    openmaintainer

github.tarball_from archive

build.pre_args-append \
    -o bin/${name} \
    -trimpath \
    -ldflags=\" -w -s \
        -X ${go.package}/cmd.version=${github.tag_prefix}${version} \
    \"

build.args          ./cmd/cli

destroot {
    xinstall -m 0755 ${worksrcpath}/bin/${name} ${destroot}${prefix}/bin/
}

