                            size    23787

depends_lib-append          port:libiconv

destroot {
    xinstall -m 0755 \
        ${worksrcpath}/target/[cargo.rust_platform]/release/${name} ${destroot}${prefix}/bin/
        file mkdir ${destroot}${prefix}/etc/${name}
        xinstall -m 644 -W ${worksrcpath} README.md SECURITY.md CONTRIBUTING.md CODE_OF_CONDUCT.md LICENSE \
            ${destroot}${prefix}/etc/${name}
}

