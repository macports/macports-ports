build.pre_args      VERSION=${version}
build.args          darwin

use_parallel_build  no

post-extract {
    reinplace "s|CGO_ENABLED=0||g" Makefile
}

destroot {
    xinstall -m 0755 ${worksrcpath}/build/${name}-darwin-amd64 \
                     ${destroot}${prefix}/bin/${name}
}

