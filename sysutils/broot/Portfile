    xinstall -m 755 ${worksrcpath}/target/[cargo.rust_platform]/release/${name} ${destroot}${prefix}/bin/
    xinstall -m 644 ${worksrcpath}/man/page ${destroot}${prefix}/share/man/man1/broot.1
}

