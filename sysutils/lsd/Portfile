PortSystem          1.0
PortGroup           cargo   1.0
PortGroup           github  1.0

github.setup        lsd-rs lsd 1.1.2 v
github.tarball_from archive
revision            0

description         The next gen ls command

long_description    {*}${description}. ${name} is heavily inspired by the \
                    super colorls project but with some little differences.

categories          sysutils
installs_libs       no
license             Apache-2
maintainers         {gmail.com:herby.gillot @herbygillot} \
                    openmaintainer

checksums           ${distname}${extract.suffix} \
                    rmd160  611609ad62dafb40160ce31abce6db87e581ad1e \
                    sha256  cd80dae9a8f6c4c2061f79084468ea6e04c372e932e3712a165119417960e14e \
                    size    108050

patchfiles-append   diff-Cargo.lock.diff

depends_build-append port:go-md2man

post-build {
    system -W ${worksrcpath} \
        "go-md2man -in ./doc/${name}.md -out ./doc/${name}.1"
}

destroot {
    xinstall -m 0755 \
        ${worksrcpath}/target/[cargo.rust_platform]/release/${name} \
        ${destroot}${prefix}/bin/

    set assets_path \
        [glob ${worksrcpath}/target/[cargo.rust_platform]/release/build/${name}-*]/out/

    xinstall -m 0444 \
        ${worksrcpath}/doc/${name}.1 ${destroot}${prefix}/share/man/man1/

    set bash_comp_path ${destroot}${prefix}/share/bash-completion/completions
    xinstall -d ${bash_comp_path}
    xinstall -m 0644 ${assets_path}/${name}.bash ${bash_comp_path}/${name}

    set zsh_comp_path ${destroot}${prefix}/share/zsh/site-functions
    xinstall -d ${zsh_comp_path}
    xinstall -m 0644 ${assets_path}/_${name} ${zsh_comp_path}

    set fish_comp_path ${destroot}${prefix}/share/fish/vendor_completions.d
    xinstall -d ${fish_comp_path}
    xinstall -m 0644 ${assets_path}/${name}.fish ${fish_comp_path}
}

notes "
You will need a patched powerline font in order for ${name} to display correctly.

See: ${homepage}#prerequisites
"

