PortSystem          1.0
PortGroup           github 1.0
PortGroup           cargo 1.0

github.setup        zellij-org zellij 0.5.0-beta v
revision            0

homepage            https://zellij.dev

description         A terminal workspace with batteries included

long_description    Zellij is a workspace aimed at developers, ops-oriented \
                    people and anyone who loves the terminal. At its core, it \
                    is a terminal multiplexer (similar to tmux and screen), \
                    but this is merely its infrastructure layer.  Zellij \
                    includes a layout system, and a plugin system allowing \
                    one to create plugins in any language that compiles to \
                    WebAssembly.

categories          sysutils
platforms           darwin
maintainers         {gmail.com:herby.gillot @herbygillot} \
                    openmaintainer
license             MIT
installs_libs       no

checksums           ${distname}${extract.suffix} \
                    rmd160  c59554154d50aa4868dc0c3b6224bd1b3634a5c2 \
                    sha256  abb7f33b5c71ea9877bb4c55632c56bc486d17be1d3b8a8791165db344c56ac7 \
                    size    4923590

set zlj_share_dir   ${prefix}/share/${name}
set zlj_compl_src   ${worksrcpath}/assets/completions

destroot {
    xinstall -m 755 \
        ${worksrcpath}/target/[cargo.rust_platform]/release/${name} \
        ${destroot}${prefix}/bin/

    xinstall -d ${destroot}${zlj_share_dir}/config

    foreach cfgfile [glob ${worksrcpath}/example/*.yaml] {
        xinstall ${cfgfile} ${destroot}${zlj_share_dir}/config/
    }

    copy ${worksrcpath}/assets/layouts ${destroot}${zlj_share_dir}/
    copy ${worksrcpath}/assets/plugins ${destroot}${zlj_share_dir}/

    xinstall -d -m 755 ${destroot}${prefix}/share/bash-completion/completions
    xinstall -m 644 ${zlj_compl_src}/${name}.bash ${destroot}${prefix}/share/bash-completion/completions/

    xinstall -d -m 755 ${destroot}${prefix}/share/fish/completions
    xinstall -m 644 ${zlj_compl_src}/${name}.fish ${destroot}${prefix}/share/fish/completions/

    xinstall -d -m 755 ${destroot}${prefix}/share/zsh/site-functions
    xinstall -m 644 ${zlj_compl_src}/_${name} ${destroot}${prefix}/share/zsh/site-functions/
}

