        ${worksrcpath}/target/[cargo.rust_platform]/release/${name} \
        ${destroot}${prefix}/bin/

    xinstall -m 0644 \
        ${worksrcpath}/docs/fselect.1 ${destroot}${prefix}/share/man/man1/
}

