destroot {
    xinstall -m 0755 ${worksrcpath}/target/[cargo.rust_platform]/release/${name} \
        ${destroot}${prefix}/bin/

    xinstall -d ${destroot}${prefix}/share/examples/${name}
    xinstall -m 0644 {*}[glob ${worksrcpath}/config/*.toml] \
        ${destroot}${prefix}/share/examples/${name}
}
