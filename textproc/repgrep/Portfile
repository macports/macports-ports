PortSystem          1.0
PortGroup           github  1.0
PortGroup           cargo   1.0

github.setup        acheronfail repgrep 0.16.0
github.tarball_from archive
revision            0

description         \
    An interactive replacer for ripgrep that makes it easy to find and \
    replace across files on the command line.

long_description    \
    ${name} is an interactive command line tool to make find and replacement \
    easy. It uses ripgrep to find, and then provides you with a simple \
    interface to see the replacements in real-time and conditionally replace \
    matches.

categories          textproc
installs_libs       no
license             Apache-2
maintainers         {gmail.com:herby.gillot @herbygillot} \
                    openmaintainer

checksums-append    ${distname}${extract.suffix} \
                    rmd160  804a85b42808228ff05f3d5b9a081343df7abbdd \
                    sha256  f7393ff98fa61d4de016670b6c5a69fff5a93f9430655f47a8eb5125d16d7fc2 \
                    size    1223130

depends_build-append \
                    port:asciidoctor
depends_run-append  port:ripgrep

notes "${name} is installed as \"rgr\""

destroot {
    set releasedir  ${worksrcpath}/target/[cargo.rust_platform]/release
    set assetsdir   {*}[glob ${releasedir}/build/repgrep-*/out]

    xinstall -m 0755 \
        ${worksrcpath}/target/[cargo.rust_platform]/release/rgr \
        ${destroot}${prefix}/bin/

    if {[file exists ${assetsdir}]} {
        xinstall -d ${destroot}${prefix}/share/man/man1
        copy ${assetsdir}/rgr.1 ${destroot}${prefix}/share/man/man1/
    }
}

