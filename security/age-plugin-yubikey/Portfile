set cargo_builddir ${worksrcpath}/target/[cargo.rust_platform]/release

post-build {
    system -W ${worksrcpath} \
        "${cargo.bin} run --example generate-docs --release ${cargo.offline_cmd} -v -j${build.jobs}"
}

destroot {
    # Install age-plugin-yubikey
    xinstall -m 0755 ${cargo_builddir}/age-plugin-yubikey ${destroot}${prefix}/bin/

    # Install man pages
    xinstall -d -m 0755 ${destroot}${prefix}/share/man/man1
    xinstall -m 0644 ${worksrcpath}/target/manpages/age-plugin-yubikey.1.gz \
        ${destroot}${prefix}/share/man/man1/

    # Install docs
    xinstall -d -m 0755 ${destroot}${prefix}/share/doc/age-plugin-yubikey
    xinstall -m 0644 ${worksrcpath}/README.md \
        ${destroot}${prefix}/share/doc/age-plugin-yubikey
}
