livecheck.type  regex
livecheck.url   http://ftp.vim.org/pub/${realname}/patches/${vim_version}/?O=D
livecheck.version [format "%s.%03d" $vim_version $vim_patchlevel]
livecheck.regex (${vim_version}\.\\d+)
