                    openmaintainer

build.env-append    GO111MODULE=off \
                    GOPROXY=off

build.cmd           make
build.target        build

installs_libs       no

destroot {
    xinstall -m 755 ${worksrcpath}/bin/fargate ${destroot}${prefix}/bin/
}

