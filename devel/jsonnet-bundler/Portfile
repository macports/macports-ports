                    -ldflags \" \
                        -s \
                        -w \
                        -extldflags "-static" \
                        -X main.Version=v${version} \
                    \" \
                    ./cmd/jb
build.env-append    CGO_ENABLED=0

destroot {
    xinstall -m 0755 ${worksrcpath}/jb ${destroot}${prefix}/bin/
}

