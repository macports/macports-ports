    xinstall -m 0755 \
        ${worksrcpath}/target/[cargo.rust_platform]/release/${name} \
        ${destroot}${prefix}/bin/

    xinstall -d ${destroot}${prefix}/share/man/man1
    xinstall -m 0644 ${worksrcpath}/Documentation/${name}.1 \
        ${destroot}${prefix}/share/man/man1/
}

