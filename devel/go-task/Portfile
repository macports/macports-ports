build.env-append    CGO_ENABLED=0
build.args          -o ${worksrcpath}/task \
                    -ldflags \" \
                        -s \
                        -w \
                        -X main.version=${version} \
                    \" \
                    ./cmd/task

destroot {
    xinstall -m 0755 ${worksrcpath}/task ${destroot}${prefix}/bin/
}

