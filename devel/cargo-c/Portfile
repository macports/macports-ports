PortSystem                  1.0
PortGroup                   github 1.0
PortGroup                   cargo  1.0
PortGroup                   openssl 1.0

github.setup                lu-zero cargo-c 0.9.5 v
revision                    0

license                     MIT
categories                  devel
platforms                   darwin
maintainers                 {mcalhoun @MarcusCalhoun-Lopez} openmaintainer

description                 cargo applet to build and install C-ABI compatibile dynamic and static libraries
long_description            {*}${description}

checksums                   ${name}-${version}${extract.suffix} \
                            rmd160  bbbdfd6df24460c257607256dd6ce6147a9fe728 \
                            sha256  13061974b9c3b1d1ff2caa852b3282cd7c3cbc835c9ca7906b6f672e7da9c495 \
                            size    28004

depends_lib-append          port:libgit2 \
                            port:libiconv \
                            port:zlib

# mimic Rust dependency
supported_archs             x86_64 arm64
universal_variant           no

pre-build {
    # Cargo.lock does not exist
    system -W ${worksrcpath} "${prefix}/bin/cargo update"
}

destroot {
    foreach bin {capi cbuild cinstall ctest} {
        xinstall -m 0755 ${worksrcpath}/target/[cargo.rust_platform]/release/cargo-${bin} ${destroot}${prefix}/bin/
    }
}

