+#include "config.h"
+
+#include "gegl.h"
+#include "gegl-plugin.h"
+
+#define SUCCESS  0
+#define FAILURE -1
+
+static int
+test_buffer_extract (void)
+{
+  gint result = SUCCESS;
+  GeglBuffer *buffer = gegl_buffer_new (GEGL_RECTANGLE (0,0,1,1),
+                                        babl_format ("R'G'B'A u8"));
+  guchar srcpix[4] = {1,2,3,4};
