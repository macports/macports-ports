+  self->index                      = NULL;
+  self->free_list                  = NULL;
+  self->next_pre_alloc             = 256; /* reserved space for header */
+  self->total                      = 256; /* reserved space for header */
+  self->map                        = NULL;
+  self->pending_ops                = 0;
